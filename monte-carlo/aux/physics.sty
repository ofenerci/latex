% physics
% physics commands, environments and macros

\ProvidesPackage{aux/physics}[05/08/2014 physics macros and commands (rimbaudcode)]

%%% Geometric objects
%
\providecommand*{\bound}{\partial} % boundary
\providecommand*{\vol}{v}          % volume

%%% Mechanics
%
\providecommand*{\mass}{m} % mass
\providecommand*{\pos}{x}  % position

%%% Transport
%
\providecommand*{\dens}{\rho} % density
\providecommand*{\flux}{f}    % flux
\providecommand*{\pnum}{n}    % particle number
%
\providecommand*{\kdiff}{\phi} % diffusivity coefficient
%
% Thermal transport
%
\providecommand*{\thtemp}{\theta} % thermodynamic temperature
\providecommand*{\kthcond}{k}     % thermal conductivity coefficient
\providecommand*{\kspthcap}{c}    % specific thermal capacity coefficient

